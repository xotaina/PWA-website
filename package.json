    "@babel/runtime": "^7.15.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^8.2.0",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^14.1.1",
    "style-loader": "^3.2.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^6.2.4"
  },
